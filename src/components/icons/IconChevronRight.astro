---
interface Props {
  width?: number;
  height?: number;

  class?: string;
  style?: string;
}
---

<svg
  class={`${Astro.props.class ?? ''}`}
  style={`${Astro.props.style ?? ''}`}
  width={Astro.props.width ?? 64}
  height={Astro.props.height ?? 64}
  xmlns="http://www.w3.org/2000/svg"
  viewBox="0 0 24 24"
  stroke-width="2"
  stroke="currentColor"
  fill="none"
  stroke-linecap="round"
  stroke-linejoin="round"
  ><path stroke="none" d="M0 0h24v24H0z" fill="none"></path><path
    d="M9 6l6 6l-6 6"></path></svg
>
